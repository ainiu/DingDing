<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".YuYueActivity" >

    <RelativeLayout
        android:id="@+id/yuyue_relativeLayout_01"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="#000000" >

        <TextView
            android:id="@+id/yuyue_text_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="预约详情"
            android:textColor="#FFFFFF"
            android:textSize="21sp" />

        <ImageView
            android:id="@+id/yuyue_image_01"
            android:layout_width="21dp"
            android:layout_height="21dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:src="@mipmap/biao" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/yuyue_relativeLayout_02"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/yuyue_relativeLayout_01" >

        <ImageView
            android:id="@+id/yuyue_image_02"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15dp"
            android:background="@mipmap/picture_33" />

        <TextView
            android:id="@+id/yuyue_text_02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/yuyue_image_02"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/yuyue_image_02"
            android:text="张师傅"
            android:textColor="#000000"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/yuyue_text_03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/yuyue_text_02"
            android:layout_centerVertical="true"
            android:text="粤**M8D1"
            android:textColor="#666666"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/yuyue_text_04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/yuyue_text_03"
            android:text="比亚迪S6"
            android:textColor="#666666"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/yuyue_image_03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/yuyue_text_03"
            android:layout_toRightOf="@+id/yuyue_text_03"
            android:src="@mipmap/yuping" />

        <TextView
            android:id="@+id/yuyue_text_05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/yuyue_image_03"
            android:layout_alignLeft="@+id/yuyue_text_04"
            android:text="4.9"
            android:textColor="#F7F7F7"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/yuyue_text_06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/yuyue_image_03"
            android:layout_alignRight="@+id/yuyue_text_04"
            android:text="185单"
            android:textColor="#666666"
            android:textSize="10sp" />

        <ImageView
            android:id="@+id/yuyue_image_04"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignBottom="@+id/yuyue_image_02"
            android:layout_alignParentRight="true"
            android:layout_marginRight="14dp"
            android:src="@mipmap/phonelin" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/yuyue_relativeLayout_03"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/yuyue_relativeLayout_02" >

        <ImageView
            android:id="@+id/yuyue_image_05"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="40dp"
            android:src="@mipmap/pingjialine" />

        <TextView
            android:id="@+id/yuyue_text_07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/yuyue_image_05"
            android:text="车费详情"
            android:textColor="#999999"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="40dp"
            android:layout_toRightOf="@+id/yuyue_text_07"
            android:src="@mipmap/pingjialine" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/yuyue_relativeLayout_04"
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/yuyue_relativeLayout_03" >

        <TextView
            android:id="@+id/yuyue_text_09"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="40dp"
            android:text="车费合计"
            android:textColor="#000000"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/yuyue_text_10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/yuyue_text_09"
            android:layout_marginLeft="120dp"
            android:text="672.5元"
            android:textColor="#333333"
            android:textSize="30sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/yuyue_relativeLayout_05"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/yuyue_relativeLayout_04" >

        <ImageView
            android:id="@+id/yuyue_image_06"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="40dp"
            android:src="@mipmap/pingjialine" />

        <TextView
            android:id="@+id/yuyue_text_08"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/yuyue_image_06"
            android:text="支付方式"
            android:textColor="#999999"
            android:textSize="12sp" />

        <ImageView
            android:id="@+id/yuyue_image_07"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="40dp"
            android:layout_toRightOf="@+id/yuyue_text_08"
            android:src="@mipmap/pingjialine" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/yuyue_relativeLayout_06"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/yuyue_relativeLayout_05" >

        <ImageView
            android:id="@+id/yuyue_image_08"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="10dp"
            android:src="@mipmap/yuwei" />

        <CheckBox
            android:id="@+id/yuyue_checkbox_01"
            style="@style/CustomCheckboxTheme"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp" />

        <TextView
            android:id="@+id/yuyue_text_11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/yuyue_image_08"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/yuyue_image_08"
            android:text="微信支付"
            android:textColor="#999999"
            android:textSize="13sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/yuyue_relativeLayout_07"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/yuyue_relativeLayout_06" >

        <ImageView
            android:id="@+id/yuyue_image_09"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="37dp"
            android:src="@mipmap/yuzhi" />

        <CheckBox
            android:id="@+id/yuyue_checkbox_02"
            style="@style/CustomCheckboxTheme"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp" />

        <TextView
            android:id="@+id/yuyue_text_12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/yuyue_image_09"
            android:layout_centerVertical="true"
            android:layout_marginLeft="9dp"
            android:layout_toRightOf="@+id/yuyue_image_09"
            android:text="支付宝支付"
            android:textColor="#999999"
            android:textSize="13sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/yuyue_relativeLayout_08"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/yuyue_relativeLayout_07" >

        <ImageView
            android:id="@+id/yuyue_image_10"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="37dp"
            android:src="@mipmap/yuxian" />

        <TextView
            android:id="@+id/yuyue_text_13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/yuyue_image_10"
            android:layout_centerVertical="true"
            android:layout_marginLeft="9dp"
            android:layout_toRightOf="@+id/yuyue_image_10"
            android:text="现金支付"
            android:textColor="#999999"
            android:textSize="13sp" />

        <CheckBox
            android:id="@+id/yuyue_checkbox_03"
            style="@style/CustomCheckboxTheme"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="35dp" />
    </RelativeLayout>

    <RelativeLayout 
        android:id="@+id/yuyue_relativeLayout_09"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_below="@+id/yuyue_relativeLayout_08"
        android:layout_marginTop="15dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        >

        <Button
            android:id="@+id/yuyue_button_01"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:background="@mipmap/yuanniu" />
        <TextView 
            android:id="@+id/yuyue_text_14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:text="确定支付672.5元"
            android:textSize="14sp"
            android:textColor="#FFFFFF"
            />

    </RelativeLayout>
    


</RelativeLayout>