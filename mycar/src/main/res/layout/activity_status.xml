<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StatusActivity" >

    <RelativeLayout
        android:id="@+id/status_relativeLayout_01"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="#000000" >

        <TextView
            android:id="@+id/status_text_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:textColor="#FFFFFF"
            android:textSize="21sp"
            android:text="身份审核" />

        <ImageView
            android:id="@+id/status_image_01"
            android:layout_width="21dp"
            android:layout_height="21dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="12dp"
            android:src="@mipmap/picture_04" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/status_relativeLayout_02"
        android:layout_width="wrap_content"
        android:layout_height="90dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/status_relativeLayout_01" >

        <ImageView
            android:id="@+id/status_image_02"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="25dp"
            android:src="@mipmap/picture_20" />

        <ImageView
            android:id="@+id/status_image_04"
            android:layout_width="21dp"
            android:layout_height="21dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="100dp"
            android:layout_alignLeft="@+id/status_image_02"
            android:src="@mipmap/picture_70" />

        <ImageView
            android:id="@+id/status_image_05"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/status_image_04"
            android:src="@mipmap/picture_71" />

        <ImageView
            android:id="@+id/status_image_06"
            android:layout_width="21dp"
            android:layout_height="21dp"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/status_image_05"
            android:src="@mipmap/picture_70" />

        <ImageView
            android:id="@+id/status_image_07"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/status_image_05"
            android:layout_toRightOf="@+id/status_image_06"
            android:src="@mipmap/picture_71" />

        <ImageView
            android:id="@+id/status_image_08"
            android:layout_width="21dp"
            android:layout_height="21dp"
            android:layout_alignTop="@+id/status_image_06"
            android:layout_toRightOf="@+id/status_image_07"
            android:src="@mipmap/picture_70" />

        <TextView
            android:id="@+id/status_text_02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/status_image_05"
            android:layout_alignTop="@+id/status_image_02"
            android:layout_marginLeft="30dp"
            android:textSize="12sp"
            android:text="审核状态" />

        <ImageView
            android:id="@+id/status_image_09"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_alignTop="@+id/status_text_02"
            android:layout_centerHorizontal="true"
            android:src="@mipmap/pictutre_88" />

    </RelativeLayout>

    <ImageView
        android:id="@+id/status_image_10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/status_relativeLayout_02"
        android:src="@mipmap/picture_84" />

    <RelativeLayout
        android:id="@+id/status_relativeLayout_03"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:background="#F6F6F6"
        android:layout_below="@+id/status_image_10" >
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/status_relativeLayout_04"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/status_relativeLayout_03" >

        <EditText
            android:id="@+id/status_edit_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/status_text_03"
            android:layout_alignBottom="@+id/status_text_03"
            android:layout_marginLeft="15dp"
            android:layout_toRightOf="@+id/status_text_03"
            android:background="@null"
            android:textColor="#E7E7E7"
            android:hint="请填写真实姓名"
            android:textSize="13sp"
            android:ems="10" >
        </EditText>

        <TextView
            android:id="@+id/status_text_03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="34dp"
            android:text="姓名"
            android:textColor="#666666"
            android:textSize="15sp" />

    </RelativeLayout>

    <ImageView
        android:id="@+id/status_image_11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/status_relativeLayout_04"
        android:layout_marginLeft="16dp"
        android:src="@mipmap/picture_90" />

    <RelativeLayout
        android:id="@+id/relativeLayout05"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/status_image_11" >

        <TextView
            android:id="@+id/status_text_04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="16dp"
            android:text="车牌号"
            android:textColor="#666666"
            android:textSize="15sp" />

        <EditText
            android:id="@+id/editText1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/status_text_04"
            android:layout_toRightOf="@+id/status_text_04"
            android:background="@null"
            android:textColor="#E7E7E7"
            android:hint="例**1234"
            android:layout_marginLeft="15sp"
            android:textSize="13sp"
            android:ems="10" >

           
        </EditText>

        <ImageView
            android:id="@+id/status_image_12"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_alignRight="@+id/editText1"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="15dp"
            android:src="@mipmap/picture_89" />

    </RelativeLayout>

    <ImageView
        android:id="@+id/status_image_13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/relativeLayout05"
        android:src="@mipmap/picture_90" />

    <RelativeLayout
        android:id="@+id/status_relativeLayout_06"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:background="#F6F6F6"
        android:layout_below="@+id/status_image_13" >
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/status_relativeLayout_07"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/status_relativeLayout_06" >
    </RelativeLayout>

    <ImageView
        android:id="@+id/status_image_14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/status_image_11"
        android:layout_alignTop="@+id/status_relativeLayout_07"
        android:src="@mipmap/picture_90" />

    <RelativeLayout
        android:id="@+id/status_relativeLayout08"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/status_image_14" >

        <TextView
            android:id="@+id/status_text_05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:text="准驾证号" 
            android:textColor="#666666"
            android:textSize="15sp" />

        <EditText
            android:id="@+id/status_edit_03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="15sp"
            android:layout_toRightOf="@+id/status_text_05"
            android:background="@null"
            android:ems="10"
            android:textColor="#E7E7E7"
            android:textSize="13sp" >

        </EditText>

        <ImageView
            android:id="@+id/status_image_15"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/status_edit_03"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="15dp"
            android:src="@mipmap/picture_89" />

  
    </RelativeLayout>

    <ImageView
        android:id="@+id/status_image_16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/status_relativeLayout08"
        android:src="@mipmap/picture_90" />

    <RelativeLayout
        android:id="@+id/status_relativeLayoue_09"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/status_image_16" >

        <TextView
            android:id="@+id/status_text_06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:text="身份证号" 
            android:textColor="#666666"
            android:textSize="15sp" />

        <EditText
            android:id="@+id/status_edit_04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/status_text_06"
            android:layout_alignBottom="@+id/status_text_06"
            android:layout_marginLeft="15dp"
            android:layout_toRightOf="@+id/status_text_06"
            android:background="@null"
            android:textColor="#E7E7E7"
            android:textSize="13sp"
            android:ems="10" >

            
        </EditText>

        <ImageView
            android:id="@+id/status_image_17"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="15dp"
            android:layout_toRightOf="@+id/status_edit_04"
            android:src="@mipmap/picture_89" />

    </RelativeLayout>
    <ImageView
        android:id="@+id/status_image_18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/status_relativeLayoue_09"
        android:src="@mipmap/picture_90" />
    <ImageView 
        android:id="@+id/status_image_19"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_above="@+id/status_button_01"
        android:src="@mipmap/picture_90"
        />
    <Button 
        android:id="@+id/status_button_01"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:background="@null"
        android:textColor="#333333"
        android:textSize="21sp"
        android:text="提交"
        />

</RelativeLayout>
