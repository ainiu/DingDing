<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
  <RelativeLayout
        android:id="@+id/daifu_myorder_relativeLayout_01"
        android:layout_width="wrap_content"
        android:layout_height="5dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="#F6F6F6" >
    </RelativeLayout>

    <ImageView
        android:id="@+id/daifu_myorder_image_01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/daifu_myorder_relativeLayout_01"
        android:src="@drawable/ding_92" />

    <RelativeLayout
        android:id="@+id/daifu_myorder_relativeLayout_02"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/daifu_myorder_image_01" >

        <ImageView
            android:id="@+id/daifu_myorder_image_02"
            android:layout_width="13dp"
            android:layout_height="13dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="14dp"
            android:src="@drawable/ding_01" />

        <TextView
            android:id="@+id/daifu_myorder_text_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="5dp"
            android:layout_toRightOf="@+id/daifu_myorder_image_02"
            android:text="布心街道"
            android:textColor="#6D6D6D"
            android:textSize="11sp" />

        <TextView
            android:id="@+id/daifu_myorder_text_02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:layout_toRightOf="@+id/daifu_myorder_text_01"
            android:text="卖家账号 : 545654532"
            android:textColor="#6D6D6D"
            android:textSize="11sp" />

        <TextView
            android:id="@+id/daifu_myorder_text_03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="14dp"
            android:text="交易成功"
            android:textColor="#1FA9EB"
            android:textSize="11sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/daifu_myorder_relativeLayout_03"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/daifu_myorder_relativeLayout_02"
        android:background="#F6F6F6" >

        <ImageView
            android:id="@+id/daifu_myorder_image_03"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="16dp"
            android:src="@drawable/ding_135" />

        <TextView
            android:id="@+id/daifu_myorder_text_08"
            android:layout_width="160dp"
            android:layout_height="30dp"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="13dp"
            android:layout_toRightOf="@+id/daifu_myorder_image_03"
            android:text="【香港直邮】极致奢华的香水! 莫杰(MarcJacobs)小皱菊女..."
            android:textColor="#666666"
            android:textSize="11sp" />

        <TextView
            android:id="@+id/daifu_myorder_text_04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/daifu_myorder_text_08"
            android:layout_marginRight="10dp"
            android:text="¥ 199.20"
            android:textColor="#666666"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/daifu_myorder_text_05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/daifu_myorder_text_04"
            android:layout_marginRight="12dp"
            android:text="×1"
            android:textColor="#999999"
            android:textSize="11sp" />

        <TextView
            android:id="@+id/daifu_myorder_text_07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/daifu_myorder_text_08"
            android:layout_below="@+id/daifu_myorder_text_05"
            android:layout_marginTop="5dp"
            android:text="颜色分类 : 黄色"
            android:textColor="#999999"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/daifu_myorder_text_06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/daifu_myorder_text_07"
            android:layout_below="@+id/daifu_myorder_text_07"
            android:text="尺寸 : 500ML"
            android:textColor="#999999"
            android:textSize="10sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/daifu_myorder_relativeLayout_04"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@id/daifu_myorder_relativeLayout_03" >

        <TextView
            android:id="@+id/daifu_myorder_text_09"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:text="（含运费¥0.00）"
            android:textColor="#696969"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/daifu_myorder_text_10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/daifu_myorder_text_09"
            android:text="¥199.20"
            android:textColor="#666666"
            android:textSize="11sp" />

        <TextView
            android:id="@+id/daifu_myorder_text_11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/daifu_myorder_text_10"
            android:text="合计 : "
            android:textColor="#696969"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/daifu_myorder_text_12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@+id/daifu_myorder_text_11"
            android:text="共1件商品"
            android:textColor="#696969"
            android:textSize="10sp" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/daifu_myorder_image_04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/daifu_myorder_relativeLayout_04"
        android:src="@drawable/ding_02" />

    <RelativeLayout
        android:id="@+id/daifu_myorder_relativeLayout_05"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/daifu_myorder_image_04" >

        

        <Button
            android:id="@+id/daifu_myorder_button_02"
            android:layout_width="60dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
           android:layout_alignParentRight="true"
            android:background="@drawable/ding_137"
            android:text="查看物流"
            android:textColor="#696969"
            android:textSize="10sp" />
    </RelativeLayout>
    
    <ImageView 
          android:id="@+id/daifu_myorder_image_05"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignParentLeft="true"
          android:layout_alignParentRight="true"
          android:layout_below="@+id/daifu_myorder_relativeLayout_05"
          android:src="@drawable/ding_92"
        />
    
</RelativeLayout>
