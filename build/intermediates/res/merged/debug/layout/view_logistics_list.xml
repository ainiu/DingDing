<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:id="@+id/view_logistics_list_relativeLayout_01"
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="#F6F6F6" >

        <ImageView
            android:id="@+id/view_logistics_list_image_01"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="13dp"
            android:src="@drawable/ding_172" />

        <TextView
            android:id="@+id/view_logistics_list_text_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="7dp"
            android:layout_marginTop="12dp"
            android:layout_toRightOf="@+id/view_logistics_list_image_01"
            android:text="圆通速递"
            android:textColor="#666666"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/view_logistics_list_text_02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/view_logistics_list_text_01"
            android:layout_below="@+id/view_logistics_list_text_01"
            android:text="运单编号 : "
            android:textColor="#666666"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/view_logistics_list_text_03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/view_logistics_list_text_02"
            android:layout_below="@+id/view_logistics_list_text_02"
            android:text="物流状态 : "
            android:textColor="#666666"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/view_logistics_list_text_04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/view_logistics_list_text_02"
            android:layout_toRightOf="@+id/view_logistics_list_text_02"
            android:text="710050331085"
            android:textColor="#666666"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/view_logistics_list_text_05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/view_logistics_list_text_04"
            android:layout_alignTop="@+id/view_logistics_list_text_03"
            android:text="签收成功"
            android:textColor="#666666"
            android:textSize="13sp" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/view_logistics_list_image_02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/view_logistics_list_relativeLayout_01"
        android:src="@drawable/ding_92" />

    <LinearLayout
        android:id="@+id/view_logitics_list_linearLayout_01"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/view_logistics_list_image_02"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/view_logistics_list_text_06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="13dp"
            android:text="物流跟踪"
            android:textColor="#666666"
            android:textSize="13sp" />
    </LinearLayout>

    <ImageView
        android:id="@+id/view_logistics_list_image_03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/view_logitics_list_linearLayout_01"
        android:src="@drawable/ding_92" />

    <RelativeLayout
        android:id="@+id/view_logistics_list_relativeLayout_02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/view_logistics_list_image_03" >

        <ImageView
            android:id="@+id/view_logistics_list_image_05"
            android:layout_width="wrap_content"
            android:layout_height="65dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="25dp"
            android:src="@drawable/ding_173" />

        <ImageView
            android:id="@+id/view_logistics_list_image_04"
            android:layout_width="14dp"
            android:layout_height="14dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="13dp"
            android:layout_marginTop="15dp"
            android:src="@drawable/ding_174" />

        <TextView
            android:id="@+id/view_logistics_list_text_07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/view_logistics_list_image_04"
            android:layout_marginLeft="25dp"
            android:layout_toRightOf="@+id/view_logistics_list_image_05"
            android:text="广东省深圳市宝安区留仙洞公司  已签收"
            android:textColor="#1FA9EB"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/view_logistics_list_text_08"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/view_logistics_list_text_07"
            android:layout_below="@+id/view_logistics_list_text_07"
            android:layout_marginTop="2dp"
            android:text="签收人 :  王"
            android:textColor="#1FA9EB"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/view_logistics_list_text_09"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/view_logistics_list_text_08"
            android:layout_below="@+id/view_logistics_list_text_08"
            android:layout_marginTop="2dp"
            android:text="2015-4-17    23:00:00"
            android:textColor="#1FA9EB"
            android:textSize="13sp" />

        <ImageView
            android:id="@+id/view_logistics_list_image_07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/view_logistics_list_text_09"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/view_logistics_list_text_09"
            android:layout_marginTop="4dp"
            android:src="@drawable/ding_176" />

        <ImageView
            android:id="@+id/view_logistics_list_image_08"
            android:layout_width="16dp"
            android:layout_height="13dp"
            android:layout_alignParentLeft="true"
            android:layout_below="@+id/view_logistics_list_image_05"
            android:layout_marginLeft="13dp"
            android:src="@drawable/ding_177" />

        <TextView
            android:id="@+id/view_logistics_list_text_10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/view_logistics_list_image_07"
            android:layout_alignTop="@+id/view_logistics_list_image_08"
            android:text="在广州龙洞已收件 进入分公司"
            android:textColor="#999999"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/view_logistics_list_text_11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/view_logistics_list_text_10"
            android:layout_below="@+id/view_logistics_list_text_10"
            android:layout_marginTop="2dp"
            android:text="2015-06-01  12:20:43"
            android:textColor="#999999"
            android:textSize="13sp" />
    </RelativeLayout>

</RelativeLayout>